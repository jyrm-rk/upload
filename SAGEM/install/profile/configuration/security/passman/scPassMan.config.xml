<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <configName>PassManConfig</configName>

    <dsConfig>
        <ClassPropertyConfig className="com.webmethods.sc.security.passman.impl.datastore.DefaultDataStore">
            <params>
                <param name="fileName" value="defaultPassStore.dat"/>
            </params>
        </ClassPropertyConfig>
     </dsConfig>
          
    <mpwConfig>
        <ClassPropertyConfig className="com.webmethods.sc.security.passman.impl.masterpw.entrust.EntrustMasterPassword">
            <params>
                <param name="fileName" value="mpw.dat"/>
            </params>   
        </ClassPropertyConfig>
    </mpwConfig>
    
    <encConfig>
        <defaultEncCode>EntrustPbePlus</defaultEncCode>
        <sanityEncCode>EntrustPbePlus</sanityEncCode>
        <encryptors>
            <ClassPropertyConfig  className="com.webmethods.sc.security.passman.impl.encryption.entrust.EntrustEncryptor">
            </ClassPropertyConfig>
        </encryptors>
    </encConfig>  
 </Config>
